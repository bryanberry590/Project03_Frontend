{
  "expo": {
    "name": "friendsync-app",
    "slug": "friendsync-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "scheme": "friendsync",
    "extra": {
      "EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID": "1013334906995-8mbvl4rhu9jh4haool5jpup9qng9q7ph.apps.googleusercontent.com"
    },
    "plugins": [
      "expo-web-browser"
    ]
  }
}
